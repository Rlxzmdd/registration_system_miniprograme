/* 验证码输入 */
.verificationCode {
  overflow: hidden;
  position: relative;
  width: 100%;
  display: flex;
  justify-content: space-between;
  padding-top: 20px;
  margin-bottom: 20rpx;
}
.verificationCode > input {
  width: calc(200%);
  position: absolute;
  left: -100px;
  top: 20px;
  height: 60px;
  background: #e9e9e9;
  opacity: 0;
  font-size: 10px;
}
.verificationCode > view {
  width: 90rpx;
  border: 1rpx solid #979797;
  height: 80rpx;
  text-align: center;
  line-height: 60rpx;
  border-radius: 14rpx;
  font-size: 70rpx;
  padding-top: 10rpx;
  color: #4DBF6E;
}
.verificationCode .activeView {
  border-color: #4DBF6E;
}
.VerifyTips{
  font-size: medium;
  padding: 10rpx 0;
  font-weight: bolder;
}
