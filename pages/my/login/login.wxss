/* 顶部信息块 */
.top-card {
  /* display: flex; */
  flex-direction: row;
  justify-content: space-between;
  min-height: 310rpx;
}

.top-card .left {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 16rpx 28rpx;
}

.top-card .left .title {
  position: relative;
  /* background-color: #d6e1ff; */
  height: 36rpx;
  width: 95%;
  margin-top: 30rpx;
}

.top-card .left .title>text {
  position: absolute;
  top: -44rpx;
  left: 14rpx;
  font-size: 60rpx;
  color: #454545;
  font-weight: 600;
}

.top-card .left .c-content {
  color: #595959;
  font-size: 60rpx;
  font-weight: bolder;
}

.top-card .left .bottom {
  font-size: 32rpx;
  color: #b5b5b5;
}

.top-card .right {
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
  align-items: center;
  width: 160rpx;
  border-left: 14rpx solid #d6e1ff;
}

.entrance text {
  display: block;
  text-align: center;
  font-size: 26rpx;
  color: #595959;
  font-weight: 600;
  padding-top: 10rpx;
}

/* CSS卡片 */
.card{/*  */
