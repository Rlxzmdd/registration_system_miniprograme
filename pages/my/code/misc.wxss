.JumpApp{
  background-color:var(--TipsColor);
  color: white;
  width: 350rpx;
  border-radius: 40rpx;
  font-size: 1.0rem;
  padding: 10rpx;
  margin: 0 auto;
  height: 50rpx;
  line-height: 50rpx;
  margin-bottom: 20rpx;
}

/* 个人信息 */
.info{
  font-size: 2rem;
  line-height: 70rpx;
  margin-bottom: 20rpx;
  font-weight: 600;
  color: #595959;
}
.personal-info{
  padding: 5rpx 10rpx;
  border-radius: 20rpx;
  background-color: white;
  margin-bottom: 20rpx;
}
.personal-info-bg{
  background-color: #f8f8f8;
    border-radius: 20rpx;
    padding: 30rpx 20rpx;
    font-size: 40rpx;
    font-weight: 600;
    color: #595959;

}

/* 核销记录 */
.record {
    display: flex;
    flex-direction: row;
    align-items: center;
  }
  
  .record .rec-item {
    background-color: #f8f8f8;
    border-radius: 20rpx;
    padding: 10rpx 20rpx;
    margin-bottom: 20rpx;
  }
  
  .rec-item .title {
    font-size: 40rpx;
    font-weight: 600;
    color: #595959;
    margin-bottom: 10rpx;
  }
  
  .rec-item .bottom {
    font-size: 32rpx;
    color: #b5b5b5;
  }
  
  .record .right {
    font-weight: 600;
    margin-right: -20rpx;
    margin-left: 8rpx;
  }
  
  /* 顶部信息块 */
.top-card .right {
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
  align-items: center;
  width: 190rpx;
  border-left: 14rpx solid #d6e1ff;
}

.entrance text {
  display: block;
  color: #595959;
  font-size: 24rpx;
  font-weight: 400;
  text-align: center;
  padding-top: 10rpx;
}
/* 遮罩 */
.wrapper {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 10rpx;
  height: 100%;
}
.s-content {/*  */
  width: 85%;
  margin: 10rpx auto 0;
  line-height: 2rem;
  padding: 40rpx 0;
}
.s-content > view >span{/*  */
  font-weight: bolder;
}

.block {
  background-color: #fff;
}
.bluebg{
  background: linear-gradient(to bottom, #000000 0%,#ffffff 100%);
}
.P_Icon{
  padding-top: 8rpx;
  width: 70rpx;
  height: 70rpx;

}